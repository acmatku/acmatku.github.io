<img src="/src/assets/images/acm_neofetch.svg" alt="ACM Neofetch"/>

<script>
  const neofetchText = document.getElementById("neofetch-text")!;
  const neofetchBody = document.getElementById("neofetch-body")!;

  neofetchBody.addEventListener("click", () => {
    neofetchBody.style.opacity = "100%";
  });

  document.addEventListener("keydown", (e) => {
    if (e.code == 'Enter') {
      neofetchBody.style.opacity = "100%";
    }
  }); 
</script>
  
